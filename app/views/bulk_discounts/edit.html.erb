<h1>Edit Bulk Discount</h1>
<%= form_with model: @bulk_discount, url: merchant_bulk_discount_path(@merchant, @bulk_discount), local: :true, method: :patch do |f| %>
  <%= f.label :percentage %>
  <%= f.text_field :percentage %>

  <%= f.label :quantity_threshold %>
  <%= f.text_field :quantity_threshold %>

  <%= f.submit "submit" %>
<% end %>
